import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long a = sc.nextLong(); 
        int b = sc.nextInt();     
        
        int count = 0;
        int rem;
        long temp = a;

        while (temp > 0) {
            rem = (int)(temp % 10);
            
            if (rem == b) {
                count++;
            }
            temp = temp / 10;
        }
         System.out.println(count);
    }
}
