import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(),rem;
        while (n > 0) {
            rem = n % 10;
            System.out.print(rem + " ");
            n = n / 10;
        }
    }
}
