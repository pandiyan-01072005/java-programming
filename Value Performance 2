import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        float b=sc.nextFloat();
        System.out.printf("%d\n",a);
        System.out.printf("%.2f\n",b);
    }
}
